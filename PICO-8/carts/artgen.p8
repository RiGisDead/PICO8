__lua__

#include colors.lua

function _init()
	cls(0)
end

function _update()
	
end

function nextrand(lo,hi)
	range = hi-lo
	r = flr(rnd(range))
	r = r + lo
	return r
end

function _draw()
	t = flr(rnd(2))
	
	x = nextrand(0,127)
	y = nextrand(0,127)
	r = nextrand(1, 20)+10
	c = flr(rnd(16))
	
	if t == 0 then
		circfill(x,y,r,c)
	else
		rectfill(x,y,r,r,c)
	end

end

