version 29
__lua__

#include colors.lua

frog_sprite = 1
frogx=-8
frogy=59
frog_anim_time=0
frog_anim_wait=.05

function _init()
	cls(0)
end

function _update()
	frogx+=1
	if frogx > 127 then frogx = -8 end
	if time() - frog_anim_time > frog_anim_wait then
		frog_sprite+=1
		frog_anim_time=time()
		if frog_sprite > 9 then frog_sprite = 1 end
	end
end

function _draw()
	cls()
	spr(frog_sprite,frogx,frogy)
	print(time())
end

